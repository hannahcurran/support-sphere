 <%- include('./partials/header') %>

<title> My Plan</title>
<h2> My Plan </h2>


<div>
  <form>
    <table>
      <tr>
        <th>What needs to happen?</th>
        <th>By When?</th>
        <th>By Who?</th>
        <th>Tick When Complete!</th>
      </tr>
      <tr>
        <td>
          <div><%= plan.what %></div>
        </td>
        <td>
          <div><%= plan.when %></div>
        </td>
        <td>
          <div><%= plan.who %></div>
        </td>
        <td>
          <div><%= plan.complete %><input id="complete" type="checkbox"></label>
          </div>
        </td>
      </tr>
    </table>
    <a href="/new"><button>Add To Plan</button></a>
  </form>
</div>


  
 <%- include('./partials/footer') %>